# MongoDB URI (you'll get this from MongoDB Atlas)
# For local development, use: mongodb://localhost:27017/bizcheck
MONGODB_URI=mongodb://localhost:27017/bizcheck

# Server Configuration
PORT=3000
NODE_ENV=development

# Queue Configuration (Anti-ban settings)
QUEUE_INTERVAL=12000
QUEUE_MAX_SIZE=20

# Cache Settings
CACHE_TTL=86400

# Email Configuration (for lead notifications - optional)
# EMAIL_SERVICE=gmail
# EMAIL_USER=your-email@gmail.com
# EMAIL_PASS=your-app-password

# Future: Add API keys when ready
# GOOGLE_PLACES_API_KEY=your_key_here
